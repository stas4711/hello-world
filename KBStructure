SQL

         SELECT Q_GetTaxList.Ech_Name, Q_GetTaxList.NameRus,
         Q_GetTaxList.NameOfCert, Q_GetTaxAuthorStatus.[Sum-Status0],
         Q_GetTaxAuthorStatus.[Sum-Status1], Q_GetTaxAuthorStatus.[Count-Status],
         Q_GetTaxList.id, Q_GetTaxList.Ech_Guid
         FROM Q_GetTaxList LEFT JOIN Q_GetTaxAuthorStatus ON (Q_GetTaxList.ID_Login
         = Q_GetTaxAuthorStatus.ID_Author) AND (Q_GetTaxList.Ech_Guid =
         Q_GetTaxAuthorStatus.Guid_Ec)
         WHERE (((Q_GetTaxList.ID_Login)=[:ID_Author]) AND











