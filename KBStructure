SQL

         SELECT Q_GetTaxAuthor.Ech_Name, Q_GetTaxAuthor.NameRus,
         Q_GetTaxAuthor.ID_Author, Q_GetTaxAuthor.NameOfCert,
         Sum(Q_GetTaxAuthor.Status0) AS [Sum-Status0], Sum(Q_GetTaxAuthor.Status1) AS
         [Sum-Status1], Count(Q_GetTaxAuthor.Status) AS [Count-Status], Q_GetTaxAuthor.id,
          Q_GetTaxAuthor.Ech_Guid
         FROM Q_GetTaxAuthor
         GROUP BY Q_GetTaxAuthor.Ech_Name, Q_GetTaxAuthor.NameRus,
         Q_GetTaxAuthor.ID_Author, Q_GetTaxAuthor.NameOfCert, Q_GetTaxAuthor.id,
         Q_GetTaxAuthor.Ech_Guid











