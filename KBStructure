SQL

         SELECT T_Logins.ID, T_Logins.Login, T_Logins.FIO, T_Logins.Email, T_Logins.Tel,
          T_Logins.Descr, T_Orgs.Name AS OrgName, T_Statuses.Status AS StatusName
         FROM (T_Logins LEFT JOIN T_Statuses ON T_Logins.Status=T_Statuses.Status_ID)
         LEFT JOIN T_Orgs ON T_Logins.ID_Org=T_Orgs.ID;











