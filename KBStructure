SQL

         SELECT T_QVersions.Status, T_QVersions.ID_Author,
         T_ExamsContentHeaders.Ech_Name, T_ExamList.NameRus,
         IIf(T_QVersions.Status=0,1,0) AS Status0, IIf(T_QVersions.Status=1,1,0) AS Status1,
         T_CertKinds.NameOfCert, T_ExamList.id, T_ExamsContentHeaders.Ech_Guid
         FROM T_CertKinds INNER JOIN ((((T_Questions INNER JOIN T_QVersions ON
         (T_Questions.LastVer=T_QVersions.Ver) AND
         (T_Questions.ID=T_QVersions.ID_Question)) INNER JOIN T_ExamsContentHeaders
         ON T_Questions.Guid_Ec=T_ExamsContentHeaders.Ech_Guid) INNER JOIN
         T_ExamList ON T_ExamsContentHeaders.Id_Exam=T_ExamList.id) INNER JOIN










