DateCreated:               08.09.2015 22:26:59         DefaultView:               2
DOL:                       Двоичные данные             GUID:                      {guid {57F80B05-FE00-
                                                                                  4A65-81D0-
LastUpdated:               16.10.2015 11:59:10         MaxRecords:                0

ODBCTimeout:               60                          OrderByOn:                 False
Orientation:               Слева направо               RecordLocks:               Отсутствует
RecordsAffected:           0                           RecordsetType:             Динамический набор

ReturnsRecords:            True                        Type:                      0
Updatable:                 True



SQL

         SELECT T_Questions.ID, T_QVersions.Status, T_QVersions.ID_Author,
         T_QVersions.ID_Reviewer, T_QVersions.Name, T_ExamsContentHeaders.Ech_Name,
         T_ExamList.NameRus, T_Orgs.Name, T_Questions.LastMod, T_Logins.FIO,
         T_Logins_1.FIO, IIf(T_QVersions.Status=0,1,0) AS Status0,
         IIf(T_QVersions.Status=1,1,0) AS Status1, IIf(T_QVersions.Status=2,1,0) AS Status2,
         IIf(T_QVersions.Status=3,1,0) AS Status3, IIf(T_QVersions.Status=4,1,0) AS Status4
         FROM T_Logins AS T_Logins_1 RIGHT JOIN (T_Logins RIGHT JOIN ((((T_Questions
         INNER JOIN T_QVersions ON (T_Questions.ID=T_QVersions.ID_Question) AND
         (T_Questions.LastVer=T_QVersions.Ver)) INNER JOIN T_ExamsContentHeaders ON
         T_Questions.Guid_Ec=T_ExamsContentHeaders.Ech_Guid) INNER JOIN T_ExamList
         ON T_ExamsContentHeaders.Id_Exam=T_ExamList.id) INNER JOIN (T_ExamListOrgs
         INNER JOIN T_Orgs ON T_ExamListOrgs.Orgs_Id=T_Orgs.ID) ON
         T_ExamList.id=T_ExamListOrgs.ExamList_Id) ON











