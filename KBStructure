ODBCTimeout:               60                          OrderByOn:                 False
Orientation:               Слева направо               RecordLocks:               Отсутствует
RecordsAffected:           0                           RecordsetType:             Динамический набор

ReturnsRecords:            True                        Type:                      0
Updatable:                 True



SQL

         SELECT T_EvalTemplateHeaders.Eth_Name, T_EvalTemplateHeaders.session_id,
         T_EvalTemplateHeaders.Id_Exam, T_EvalTemplateHeaders.ExamDate,
         T_ExamList.NameRus AS ExamName, T_EvalTemplateHeaders.Mnl,
         T_EvalTemplateHeaders.Mr, T_EvalTemplateHeaders.Pnl,
         T_EvalTemplateHeaders.Pr, T_EvalTemplateHeaders.G
         FROM T_EvalTemplateHeaders INNER JOIN T_ExamList ON
         T_EvalTemplateHeaders.Id_Exam=T_ExamList.id










