SQL

         SELECT T_Questions.ID, T_QVersions.ID_Question, T_QVersions.Ver,
         T_QVersions.ID_Author, T_QVersions.Name, T_QVersions.Descr, T_QVersions.Score,
         T_QVersions.Simplicity, T_Questions.ExamsContent_Ec_Code,
         T_Questions.ID_Order, T_Questions.LastVer, T_Questions.LastMod,
         T_Questions.LastModBy, T_Questions.ID_Structure, T_Questions.Guid_Ec
         FROM T_Questions INNER JOIN T_QVersions ON (T_Questions.LastVer =
         T_QVersions.Ver) AND (T_Questions.ID = T_QVersions.ID_Question);











