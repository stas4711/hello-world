SQL

         SELECT T_ModuleQuestions.ID, T_ModuleQuestions.ID_Version, T_QVersions.ID,
         T_QVersions.Name, T_Answers.ID, T_Answers.ID_Version, T_Answers.Letter,
         T_Answers.Name, T_Answers.correct_answer
         FROM T_Answers INNER JOIN (T_ModuleQuestions INNER JOIN T_QVersions ON
         T_ModuleQuestions.ID_Version = T_QVersions.ID) ON (T_Answers.ID_Version =
         T_QVersions.ID) AND (T_Answers.ID_Version = T_QVersions.ID)
         ORDER BY T_ModuleQuestions.ID, T_Answers.ID;











