SQL

         SELECT T_ExamsContentHeaders.Ech_Name, T_ExamList.NameRus,
         T_CertKinds.NameOfCert, T_ExamList.id, T_ExamsContentHeaders.Ech_Guid,
         T_Accounts.ID_Login
         FROM (T_CertKinds INNER JOIN ((T_ExamsContentHeaders INNER JOIN T_ExamList
         ON T_ExamsContentHeaders.Id_Exam=T_ExamList.id) INNER JOIN T_CertExams ON
         T_ExamList.id=T_CertExams.id_exam) ON T_CertKinds.ID=T_CertExams.id_cert)
         INNER JOIN T_Accounts ON T_ExamList.id=T_Accounts.ID_Test;











